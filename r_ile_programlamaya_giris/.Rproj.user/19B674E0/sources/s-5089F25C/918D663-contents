a <- sqrt(9) #karekok
b <- abs(-19) #mutlak deger
a*b #iki degerin carpilmasi
c <- a*b #carpilan ifadenin kaydedilmesi 
(c <- a*b) #hem kayit hem de consol ciktisi
(d <- a*b) 
